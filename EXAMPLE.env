# Environment settings
LLM="VertexAIGemini"  # OpenAI | AzureOpenAI | VertexAIGemini | BedrockTitan | BedrockClaude | BedrockLlama | BedrockMistral | BedrockNova | BedrockDeepseek
CODEBASE_DIR_PATH="/home/myname/Projects/sampleapp"
MONGODB_URL="mongodb+srv://myuser:mypassword@mycluster.abc123.mongodb.net/"
IGNORE_ALREADY_PROCESSED_FILES=false

# OpenAI API specific variables
OPENAI_LLM_API_KEY="xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx"
OPENAI_EMBEDDINGS_MODEL="text-embedding-3-small"
OPENAI_COMPLETIONS_MODEL_PRIMARY="gpt-4o"
OPENAI_COMPLETIONS_MODEL_SECONDARY="gpt-4-turbo"

# Azure OpenAI API specific variables
AZURE_LLM_API_KEY="yyyyyyyyyyyyyyyyyyyyyyyyyyyyyyyy"
AZURE_API_ENDPOINT="https://myendpoint.openai.azure.com/"
AZURE_API_EMBEDDINGS_MODEL="MyEmbeddingsModel"
AZURE_API_COMPLETIONS_MODEL_PRIMARY="MyPrimaryCompletionsModel"
AZURE_API_COMPLETIONS_MODEL_SECONDARY="MySecondaryCompletionsModel"
AZURE_OPENAI_EMBEDDINGS_MODEL_URN="text-embedding-ada-002"
AZURE_OPENAI_COMPLETIONS_MODEL_PRIMARY_URN="gpt-4o"
AZURE_OPENAI_COMPLETIONS_MODEL_SECONDARY_URN="gpt-4-turbo"

# GCP VertexAI API specific variables (see readme for how to configure authentication)
GCP_API_PROJECTID="my-gcp-project"
GCP_API_LOCATION="us-central1"
VERTEXAI_EMBEDDINGS_MODEL="text-embedding-005"
VERTEXAI_COMPLETIONS_MODEL_PRIMARY="gemini-2.5-pro-preview-05-06"
VERTEXAI_COMPLETIONS_MODEL_SECONDARY="gemini-2.0-flash-001"

# AWS Bedrock API specific variables (see README.md for how to configure authentication & region)
